program RecursiveFib is

global variable x : integer;
variable i : integer;
variable max : integer;
variable out: bool;

procedure Fib : integer(variable val :integer)
    variable ret : integer;
    variable tmp : integer[2];

	procedure Sub : integer(variable val1 : integer)
        variable val2 : integer;
		begin
			val2 := val2 - val1;
			return val2;
		end procedure;

	begin
	if(val == 0) then
		ret := 0;
		return ret;
	end if;
	if(val == 1) then
		ret := 1;
		return ret;
	end if; // Remove line
	val := Sub(1);
	tmp[0] := Fib(val);
	val := Sub(1);
	tmp[1] := Fib(val);
	ret := tmp[0] + tmp[1];
	return ret;
end procedure;


begin

max := getInteger();
for(i := 0; i < max)
	x := Fib(i);
    out := putInteger(x);
	i := i + 1;
end for;

end program.
