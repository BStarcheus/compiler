program arr is
    variable tmp : integer;
    variable arr : string[3];
    variable arr2: string[3];
    variable res : bool[3];

    procedure printArrStr: integer(variable x : string[3])
        variable ind : integer;
    begin
        for (ind := 0; ind < 3)
            tmp := putstring(x[ind]);
            ind := ind + 1;
        end for;
	return 0;
    end procedure;

    procedure printArrBool: integer(variable x : bool[3])
        variable ind : integer;
    begin
        for (ind := 0; ind < 3)
            tmp := putbool(x[ind]);
            ind := ind + 1;
        end for;
	return 0;
    end procedure;

begin
    arr[0] := "a";
    arr[1] := "b";
    arr[2] := "c";
    arr2[0] := "A";
    arr2[1] := "B";
    arr2[2] := "c";

    // Creates a bool array, whether each element is equal
    tmp := printArrBool(arr == arr2);

    // Update each element
    arr := arr2;
    tmp := printArrStr(arr);
    tmp := printArrStr(arr2);
end program.
